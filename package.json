{
  "name": "card-game-analysis",
  "version": "1.0.0",
  "description": "Real-time card game analysis system",
  "scripts": {
    "start:backend": "ts-node --esm backend/app.ts",
    "start:frontend": "cd frontend && next dev",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "nodemon --exec ts-node --esm backend/app.ts",
    "test": "jest",
    "worker": "ts-node --esm backend/workers/analysis.worker.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "ws": "^8.14.2",
    "dotenv": "^16.3.1",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "chart.js": "^4.4.0",
    "zustand": "^4.4.6",
    "tailwindcss": "^3.3.5",
    "bull": "^4.12.0",
    "ioredis": "^5.3.2",
    "python-shell": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/pg": "^8.10.9",
    "@types/ws": "^8.5.10",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.38",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.10"
  }
}
